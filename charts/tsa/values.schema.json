{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "forceNamespace": {
            "type": "string"
        },
        "namespace": {
            "type": "object",
            "properties": {
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "server": {
            "type": "object",
            "properties": {
                "args": {
                    "type": "object",
                    "properties": {
                        "cert_chain": {
                            "type": "null"
                        },
                        "kms_key_resource": {
                            "type": "null"
                        },
                        "port": {
                            "type": "integer"
                        },
                        "signer": {
                            "type": "string"
                        },
                        "tink_enc_keyset": {
                            "type": "null"
                        },
                        "tink_hcvault_token": {
                            "type": "null"
                        },
                        "tink_key_resource": {
                            "type": "null"
                        }
                    }
                },
                "grpcSvcPort": {
                    "type": "integer"
                },
                "image": {
                    "type": "object",
                    "properties": {
                        "pullPolicy": {
                            "type": "string"
                        },
                        "registry": {
                            "type": "string"
                        },
                        "repository": {
                            "type": "string"
                        },
                        "version": {
                            "type": "string"
                        }
                    }
                },
                "ingress": {
                    "type": "object",
                    "properties": {
                        "http": {
                            "type": "object",
                            "properties": {
                                "annotations": {
                                    "type": "object"
                                },
                                "className": {
                                    "type": "string"
                                },
                                "enabled": {
                                    "type": "boolean"
                                },
                                "hosts": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "host": {
                                                "type": "string"
                                            },
                                            "path": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                },
                                "tls": {
                                    "type": "array"
                                }
                            }
                        }
                    }
                },
                "logging": {
                    "type": "object",
                    "properties": {
                        "production": {
                            "type": "boolean"
                        }
                    }
                },
                "name": {
                    "type": "string"
                },
                "replicaCount": {
                    "type": "integer"
                },
                "secret": {
                    "type": "string"
                },
                "securityContext": {
                    "type": "object",
                    "properties": {
                        "runAsNonRoot": {
                            "type": "boolean"
                        },
                        "runAsUser": {
                            "type": "integer"
                        }
                    }
                },
                "service": {
                    "type": "object",
                    "properties": {
                        "ports": {
                            "type": "array",
                            "items": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "port": {
                                        "type": "integer"
                                    },
                                    "protocol": {
                                        "type": "string"
                                    },
                                    "targetPort": {
                                        "type": "integer"
                                    }
                                }
                            }
                        },
                        "type": {
                            "type": "string"
                        }
                    }
                },
                "serviceAccount": {
                    "type": "object",
                    "properties": {
                        "annotations": {
                            "type": "object"
                        },
                        "create": {
                            "type": "boolean"
                        },
                        "mountToken": {
                            "type": "boolean"
                        },
                        "name": {
                            "type": "string"
                        }
                    }
                },
                "svcPort": {
                    "type": "integer"
                }
            }
        }
    }
}
